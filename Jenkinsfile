pipeline {
  
       agent any
  stages{
    stage("build"){
      steps{
        echo 'building the application from after checkout from branch..'
      }
    }
    stage("test"){
      steps{
        echo 'testing the application..'
      }
    }
    stage("deploy"){
      steps{
        echo 'deploying the application..'
      }
    }
  }
}
